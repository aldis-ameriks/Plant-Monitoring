{
  "name": "plant-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-common": "3.1.3",
    "@apollo/react-hooks": "3.1.3",
    "apexcharts": "3.10.1",
    "apollo-boost": "0.4.4",
    "date-fns": "2.6.0",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "moving-averages": "4.0.5",
    "react": "16.11.0",
    "react-apexcharts": "1.3.3",
    "react-apollo": "3.1.3",
    "react-apollo-hooks": "0.5.0",
    "react-dom": "16.11.0",
    "react-ga": "2.7.0",
    "react-router-dom": "5.1.2",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-operations": "1.8.3",
    "@graphql-codegen/typescript-react-apollo": "1.8.3",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.6",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-router-dom": "5.1.2",
    "@types/styled-components": "4.1.20",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-typescript": "4.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "graphql-codegen": "0.4.0",
    "prettier": "1.18.2",
    "react-scripts": "3.2.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o ../results/eslint/result.xml",
    "generate": "gql-gen && eslint --fix src/graphql.tsx && prettier --write src/graphql.tsx",
    "docker:build": "docker build -t registry.kataldi.com/plant-client .",
    "docker:push": "docker push registry.kataldi.com/plant-client",
    "docker:build:push": "yarn docker:build && yarn docker:push",
    "docker:run": "docker run -p 80:80 --rm registry.kataldi.com/plant-client"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
