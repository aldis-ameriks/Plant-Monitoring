    "graphql-yoga": "^1.18.0",
    "influx": "^5.2.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.10",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
