{
  "NewDeviceInput": {
    "properties": {
      "id": {
        "type": "number"
      },
      "name": {
        "maxLength": 255,
        "type": "string"
      },
      "room": {
        "maxLength": 255,
        "type": "string"
      },
      "firmware": {
        "maxLength": 255,
        "type": "string"
      }
    },
    "type": "object",
    "required": ["id", "name", "room", "firmware"]
  },
  "PairDeviceInput": {
    "properties": {
      "version": {
        "maxLength": 20,
        "type": "string"
      }
    },
    "type": "object",
    "required": ["version"]
  },
  "BaseError": {
    "properties": {
      "statusCode": {
        "type": "number"
      },
      "error": {
        "type": "string"
      },
      "message": {
        "type": "string"
      }
    },
    "type": "object",
    "required": ["statusCode", "error", "message"]
  },
  "Notification": {
    "properties": {
      "id": {
        "type": "string"
      },
      "title": {
        "type": "string"
      },
      "body": {
        "type": "string"
      },
      "type": {
        "enum": ["low_battery", "low_moisture"],
        "type": "string"
      }
    },
    "type": "object",
    "required": ["id", "title", "body", "type"]
  },
  "NotificationResponse": {
    "properties": {
      "data": {
        "items": {},
        "type": "array"
      }
    },
    "type": "object",
    "required": ["data"]
  },
  "ReadingInput": {
    "properties": {
      "device_id": {
        "minLength": 1,
        "maxLength": 6,
        "type": "string"
      },
      "reading_id": {
        "maximum": 65535,
        "type": "number",
        "minimum": 0
      },
      "moisture": {
        "maximum": 100,
        "type": "number",
        "minimum": 0
      },
      "moisture_raw": {
        "maximum": 10000,
        "type": "number",
        "minimum": 0
      },
      "moisture_min": {
        "maximum": 10000,
        "type": "number",
        "minimum": 0
      },
      "moisture_max": {
        "maximum": 10000,
        "type": "number",
        "minimum": 0
      },
      "temperature": {
        "maximum": 100,
        "type": "number",
        "minimum": -100
      },
      "battery_voltage": {
        "exclusiveMinimum": true,
        "minimum": 0,
        "type": "number"
      },
      "light": {
        "minimum": 0,
        "type": "number"
      },
      "signal": {
        "type": "number"
      },
      "firmware": {
        "minLength": 1,
        "maxLength": 10,
        "type": "string"
      }
    },
    "type": "object",
    "required": [
      "device_id",
      "reading_id",
      "moisture",
      "moisture_raw",
      "moisture_min",
      "moisture_max",
      "temperature",
      "battery_voltage",
      "signal"
    ]
  },
  "UserSetting": {
    "properties": {
      "name": {
        "maxLength": 255,
        "type": "string"
      },
      "value": {
        "maxLength": 255,
        "type": "string"
      }
    },
    "type": "object",
    "required": ["name", "value"]
  },
  "UserSettingInput": {
    "properties": {
      "name": {
        "maxLength": 255,
        "type": "string"
      },
      "value": {
        "maxLength": 255,
        "type": "string"
      }
    },
    "type": "object",
    "required": ["name", "value"]
  },
  "LoginResponse": {
    "properties": {
      "accessKey": {
        "maxLength": 256,
        "type": "string"
      }
    },
    "type": "object",
    "required": ["accessKey"]
  },
  "LoginInput": {
    "properties": {
      "accessKey": {
        "maxLength": 256,
        "type": "string"
      }
    },
    "type": "object",
    "required": ["accessKey"]
  }
}
